<h2>Articles</h2>
<p>..........</p>
<div class="containter">
  <% @articles.each do |article| %>
    <div class="card">
      <div class="card-body">
        <h3 class="card-title"><%= link_to article.title, article_path(article) %></h3>
        <p class="card-text"><%= article.content %></p>
      </div>
    </div>
  <% end %>
</div>
